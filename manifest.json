{
  "manifest_version": 2,

  "name": "Texditor",
  
  "description": "This extension allows user to edit LaTeX without leaving the tab",
  
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}