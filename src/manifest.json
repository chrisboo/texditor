{
  "manifest_version": 2,

  "name": "texditor",
  
  "description": "Easily type your math and get its LaTeX.",
  
  "version": "0.0.0.2",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}